-- Adicionando a funcionalidade para o bot√£o de "Equipar Skin"
local Button4 = AddButton(Tab4, {
    Name = "üëª‚ò†Ô∏èSkin Do Grupo üëª‚ò†Ô∏è", 
    Callback = function()
        applySkin()
    end
})

