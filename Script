permissionButton.TextColor3 = Color3.fromRGB(255, 255, 255)
permissionButton.Parent = Tab2

permissionButton.MouseButton1Click:Connect(function()
    local nome = nomeInput.Text
    local casa = tonumber(casaInput.Text)
    local targetPlayer = game.Players:FindFirstChild(nome)

    if targetPlayer then
        local args = {
            [1] = "GivePermissionLoopToServer",
            [2] = targetPlayer,
            [3] = casa
        }

        local event = game:GetService("ReplicatedStorage").RE:FindFirstChild("1Playe1rTrigge1rEven1t")
        if event then
            event:FireServer(unpack(args))
        end
    end
end)

-- Adicionando o checkbox "Nome para todo servidor" dentro de Tab5 (‚ò†Ô∏è nome all ‚ò†Ô∏è)
local checkbox = Instance.new("TextButton")
checkbox.Size = UDim2.new(0, 200, 0, 40)
checkbox.Position = UDim2.new(0, 10, 0, 10)
checkbox.Text = "‚ò†Ô∏è Nome para todo servidor ‚ò†Ô∏è"
checkbox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
checkbox.TextColor3 = Color3.fromRGB(0, 0, 0)
checkbox.Parent = Tab5

local isChecked = false
local teleporting = false  -- Flag to control teleportation
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- Fun√ß√£o para teleportar para a cabe√ßa dos outros jogadores com um intervalo muito curto (mil√©simos de segundo)
local function teleportFollow()
    while teleporting do
        -- Itera sobre todos os jogadores no servidor
        for _, player in pairs(Players:GetPlayers()) do
            -- Verifica se o player n√£o √© o jogador local e se o jogador alvo tem a cabe√ßa
            if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("Head") then
                -- Teleportar o jogador local para a cabe√ßa do outro jogador
                LocalPlayer.Character:SetPrimaryPartCFrame(player.Character.Head.CFrame + Vector3.new(0, 3, 0)) -- Teleporta acima da cabe√ßa do jogador
                wait(0.05) -- Intervalo de 50 milissegundos entre os teleports
            end
        end
    end
end

-- Modificando a checkbox para ativar/desativar a fun√ß√£o
checkbox.MouseButton1Click:Connect(function()
    isChecked = not isChecked
    if isChecked then
        checkbox.Text = "‚úîÔ∏è Nome para ativado"
        teleporting = true  -- Ativa o teleportamento
        teleportFollow()  -- Chama a fun√ß√£o para come√ßar a teleporta√ß√£o
        print("Nome para ativado")
    else
        checkbox.Text = "‚ò†Ô∏è Nome para desativado ‚ò†Ô∏è"
        teleporting = false  -- Desativa o teleportamento
        print("Nome para desativado")
    end
end)

