-- Fun√ß√£o para aplicar a skin
local function applySkin()
    local event = game:GetService("ReplicatedStorage").RE:FindFirstChild("1Avata1rOrigina1l")
    
    if event then
        local args1 = {
            [1] = "CharacterChange",
            [2] = {0, 0, 0, 0, 0, 0},
            [3] = "AllBlocky"
        }
        event:FireServer(unpack(args1))
        wait(1)

        local args2 = {
            [1] = "OCA"
        }
        event:FireServer(unpack(args2))
        wait(1)
    end

    local eventUpdate = game:GetService("ReplicatedStorage").RE:FindFirstChild("1Updat1eAvata1r")
    if eventUpdate then
        local items = {
            18504846414,
            101242044554922,
            63690008,
            13722734227,
            8993037050,
            8993041085,
            15428251511
        }

        for _, item in ipairs(items) do
            local args = {
                [1] = "wear",
                [2] = item
            }
            eventUpdate:FireServer(unpack(args))
            wait(1)
        end

        local walkStyles = {
            5319922112,
            5319909330,
            5319900634,
            5319917561
        }

        for _, walkStyle in ipairs(walkStyles) do
            local args = {
                [1] = "wearWalkStyle",
                [2] = walkStyle
            }
            eventUpdate:FireServer(unpack(args))
            wait(1)
        end

        -- Definir tom de pele (skintone) novamente no final
        local skinToneArgs = {
            [1] = "skintone",
            [2] = "Brown"
        }
        eventUpdate:FireServer(unpack(skinToneArgs))
        wait(1)

        -- √öltima parte da skin que voc√™ pediu para adicionar
        local finalArgs = {
            [1] = "CharacterChange",
            [2] = {0, 0, 0, 0, 0, 0},
            [3] = "AllBlocky"
        }
        event:FireServer(unpack(finalArgs))

        -- Definir o tom de pele mais uma vez ap√≥s o √∫ltimo "CharacterChange"
        eventUpdate:FireServer(unpack(skinToneArgs))

        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "Skin Completa!",
            Text = "Bem-vindo √† Team Hk",
            Duration = 5
        })
    end
end

