local ScreenGui = Instance.new("ScreenGui")
local ImageLabel = Instance.new("ImageLabel")

ScreenGui.Parent = game.CoreGui -- Adiciona a GUI Ã  interface do jogo

ImageLabel.Parent = ScreenGui
ImageLabel.Size = UDim2.new(0.5, 0, 0.5, 0) -- 50% da tela
ImageLabel.Position = UDim2.new(0.25, 0, 0.25, 0) -- Centraliza
ImageLabel.BackgroundTransparency = 1 -- Sem fundo

-- COLOQUE A URL DA IMAGEM AQUI
ImageLabel.Image = "https://uploaddeimagens.com.br/imagens/b8ZoQ5k/image.png" 

-- Remove ao reiniciar
game:GetService("Players").LocalPlayer.CharacterAdded:Connect(function()
    ScreenGui:Destroy()
end)
